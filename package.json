{
  "name": "@expo/rudder-sdk-node",
  "version": "1.0.8",
  "description": "Compact fork of rudder-node-sdk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/rudder-sdk-node.git"
  },
  "author": "Expo",
  "engines": {
    "node": ">=4"
  },
  "size-limit": [
    {
      "limit": "25 KB",
      "path": "index.js"
    }
  ],
  "scripts": {
    "dependencies": "yarn",
    "size": "size-limit",
    "build": "babel index.js --out-dir lib && cp package.json lib/ && cp readme.md lib/ && cp History.md lib/ && cp LICENSE.md lib/ && cp index.d.ts lib/",
    "test": "nyc --reporter=lcov --reporter=html --reporter=text ava --serial --verbose  > coverage.lcov",
    "lint": "eslint . --fix",
    "changelog": "auto-changelog -p -t keepachangelog -u true -l false --sort-commits date-desc ",
    "tsc": "tsc"
  },
  "main": "index.js",
  "keywords": [
    "analytics"
  ],
  "dependencies": {
    "@expo/bunyan": "^4.0.0",
    "@segment/loosely-validate-event": "^2.0.0",
    "fetch-retry": "^4.1.1",
    "lodash.isstring": "^4.0.1",
    "md5": "^2.2.1",
    "ms": "^2.0.0",
    "node-fetch": "^2.6.1",
    "remove-trailing-slash": "^0.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "auto-changelog": "^1.16.2",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.17.1",
    "codecov": "^3.0.0",
    "commander": "^2.9.0",
    "delay": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-universe": "^7.0.1",
    "express": "^4.15.2",
    "husky": "^3.0.4",
    "nyc": "^14.1.1",
    "pify": "^4.0.1",
    "prettier": "^2.3.2",
    "sinon": "^7.3.2",
    "size-limit": "^1.3.5",
    "snyk": "^1.171.1",
    "standard": "^12.0.1",
    "typescript": "^4.3.5"
  }
}

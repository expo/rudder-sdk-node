{
    "include": ["index.ts"],
    "extends": "@tsconfig/node12/tsconfig.json",
    "compilerOptions": {
      "declaration": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "useUnknownInCatchVariables": false
    },
}
